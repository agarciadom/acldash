     },
     {
      "name": "clickAction",
      "value": "function f(ev) {\n\n    if (ev.category == 'path') {\n        var viewURL = artifactCatalog.getViewURL(ev.value);\n        window.open(viewURL);\n    }\n    else if (ev.category == 'resourceID') {\n        var editURL;\n        if (ev.value !== null) {\n            editURL = 'resourceform?resourceID=' + ev.value;\n        } else {\n            editURL = 'resourceform?url=' + ev.series;\n        }\n        window.open(editURL);\n    }\n    else if (ev.category == 'isVisible') {\n        artifactCatalog.runEndpoint('togglePermission', {\n        params: {\n            'fileName': ev.series,\n            'user': Dashboards.getParameterValue(\"targetUser\"),//targetUser,  //Dashboards.getParameterValue(\"targetRole\"), \n            'visible': ev.value,\n            'roles': Dashboards.getParameterValue(\"rolesUsers\"),//rolesUsers,\n        },\n          success: function (data) {\n            var statusHTML=\"\";\n\n            if(Dashboards.getParameterValue(\"targetUser\") == 'admin') {\n                statusHTML = Dashboards.i18nSupport.prop(\"error.cannot_revoke\");\n            } else {\n                if (data.resultset[0][0] !== '') {\n                    statusHTML = Dashboards.i18nSupport.prop(\"error.role_access\") + \": \"+ data.resultset[0];\n                }\n                else {\n                    // Generar mensaje de estado y actualizar tabla\n                    statusHTML = Dashboards.i18nSupport.prop(\"success.permissions\");\n\n                    var dataTable = ev.target.closest(\"table\").dataTable();\n                    var oldValue = ev.target.text();\n                    var newValue = oldValue === \"true\" ? \"false\" : \"true\";\n                    dataTable.fnUpdate(newValue, ev.rowIdx, ev.colIdx);\n                }\n            }\n            var statusPanel = $(\"#statusPanel\");\n            statusPanel.stop(true, true);\n            statusPanel.text(statusHTML).show().fadeOut(3000);\n        }\n    });\n    \n    }\n} ",
      "type": "JavaScript"
     }
    ]
