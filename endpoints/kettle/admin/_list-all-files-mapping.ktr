        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.io.StringReader;

import javax.xml.parsers.DocumentBuilderFactory;

import org.w3c.dom.Document;
import org.w3c.dom.Element;
import org.w3c.dom.NodeList;
import org.xml.sax.InputSource;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}
    if (first)
    {
        first = false;
    }

    final String sXML = get(Fields.In, "result").getString(r);
    try {
        final Document doc =
            DocumentBuilderFactory.newInstance().newDocumentBuilder()
                .parse(new InputSource(new StringReader(sXML)));

        final NodeList aElems = doc.getElementsByTagName("file");
        for (int iElem = 0; iElem < aElems.getLength(); ++iElem) {
            final Element e = (Element)aElems.item(iElem);

            boolean isFolder = false;
            String path = null;

            final NodeList pathElems = e.getElementsByTagName("path");
            if (pathElems.getLength() > 0) {
                path = pathElems.item(0).getTextContent();
            }

            final NodeList isFolderElems = e.getElementsByTagName("folder");
            if (isFolderElems.getLength() > 0) {
                isFolder = "true".equals(isFolderElems.item(0).getTextContent());
            }

            if (path != null) {
                // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
                // enough to handle any new fields you are creating in this step.
                r = createOutputRow(r, 1);

                get(Fields.Out, "isFolder").setValue(r, isFolder);
                get(Fields.Out, "path").setValue(r, path);

                // Send the row on to the next step.
                putRow(data.outputRowMeta, r);
            }
		}
    }
    catch (Exception e) {
        throw new KettleException(e);
    }

	return true;
}
]]></class_source>
