    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#x2f;&#x2f; Roles that are already granting the permission to access the file&#xa;&#x2f;&#x2f; &#x28;toggling access on a user that has access already through a role&#xa;&#x2f;&#x2f; won&#x27;t do anything&#x29;.&#xa;var rolesPermission &#x3d; &#x27;&#x27;&#x3b;&#xa;&#xa;var USER_TYPE &#x3d; 0&#x3b;&#xa;var ROLE_TYPE &#x3d; 1&#x3b;&#xa;var disable_permission &#x3d; function &#x28;xml, target, recipientType&#x29; &#x7b;&#xa;  return xml.replace&#x28;&#x22;&#x3c;aces&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;modifiable&#x3e;true&#x3c;&#x2f;modifiable&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;permissions&#x3e;0&#x3c;&#x2f;permissions&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;recipient&#x3e;&#x22; &#x2b; target &#x2b; &#x22;&#x3c;&#x2f;recipient&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;recipientType&#x3e;&#x22; &#x2b; recipientType &#x2b; &#x22;&#x3c;&#x2f;recipientType&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;&#x2f;aces&#x3e;&#x22;, &#x22; &#x22;&#x29;&#x3b;&#xa;&#x7d;&#x3b;&#xa;var enable_permission &#x3d; function &#x28;xml, target, recipientType&#x29; &#x7b;&#xa;  return xml.replace&#x28;&#x22;&#x3c;repositoryFileAclDto&#x3e;&#x22;,&#xa;    &#x22;&#x3c;repositoryFileAclDto&#x3e;&#x3c;aces&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;permissions&#x3e;0&#x3c;&#x2f;permissions&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;recipient&#x3e;&#x22; &#x2b; target &#x2b; &#x22;&#x3c;&#x2f;recipient&#x3e;&#x22;&#xa;    &#x2b; &#x22;&#x3c;recipientType&#x3e;&#x22; &#x2b; recipientType &#x2b; &#x22;&#x3c;&#x2f;recipientType&#x3e;&#x3c;&#x2f;aces&#x3e;&#x22;&#x29;&#x3b;&#xa;&#x7d;&#xa;&#xa;var type &#x3d; &#x28;userRoles &#x21;&#x3d; &#x27;&#x27;&#x29; &#x3f; USER_TYPE &#x3a; ROLE_TYPE&#x3b;&#xa;&#xa;if &#x28;visible &#x3d;&#x3d; &#x22;true&#x22;&#x29; &#x7b;&#xa;  var permission &#x3d; disable_permission&#x28;result, target, type&#x29;&#x3b;&#xa;&#xa;  if &#x28;type &#x3d;&#x3d; USER_TYPE&#x29; &#x7b;&#xa;    var nroles &#x3d; userRoles.split&#x28;&#x22;,&#x22;&#x29;&#x3b;	&#xa;	for &#x28;var i&#x3d;0&#x3b; i &#x3c; nroles.length&#x3b; i&#x2b;&#x2b;&#x29; &#x7b;&#xa;		if &#x28;result.indexOf&#x28;nroles&#x5b;i&#x5d;&#x29; &#x21;&#x3d; -1&#x29; &#x7b;&#xa;			rolesPermission &#x2b;&#x3d; nroles&#x5b;i&#x5d;&#x3b;&#xa;		&#x7d;&#xa;	&#x7d;&#xa;  &#x7d;&#xa;&#x7d; else &#x7b;&#xa;  var permission &#x3d; enable_permission&#x28;result, target, type&#x29;&#x3b;&#xa;&#x7d;&#xa;&#xa;&#x2f;&#x2f; Disable permission inheritance &#x28;otherwise, changes won&#x27;t be effective&#x29;&#xa;permission &#x3d; permission.replace&#x28;&#xa;  &#x22;&#x3c;entriesInheriting&#x3e;true&#x3c;&#x2f;entriesInheriting&#x3e;&#x22;,&#xa;  &#x22;&#x3c;entriesInheriting&#x3e;false&#x3c;&#x2f;entriesInheriting&#x3e;&#x22;&#x29;&#x3b;</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>permission</name>
        <rename>permission</rename>
        <type>String</type>
        <length>-1</length>
        <precision>-1</precision>
        <replace>N</replace>
